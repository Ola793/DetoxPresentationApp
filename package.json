{
  "name": "DetoxPresentationApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "emulator": "powershell -Command \"Start-Process -FilePath $env:ANDROID_HOME\\emulator\\emulator.exe -ArgumentList '-avd','Pixel_7' -WindowStyle Hidden\"",
    "detox:build": "detox build -c android.emu.debug",
    "detox:test": "detox test -c android.emu.debug --runInBand",
    "detox:test:starter": "detox test -c android.emu.debug e2e/01-starter.test.js",
    "detox:test:positive": "detox test -c android.emu.debug e2e/02-positive.e2e.test.js",
    "detox:test:negative": "detox test -c android.emu.debug e2e/03-negative.e2e.test.js",
    "detox:test:interaction": "detox test -c android.emu.debug e2e/04-input-interaction.e2e.test.js",
    "open:report": "npx open-cli test-report.html"
  },
  "dependencies": {
    "react": "18.3.1",
    "react-native": "0.76.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "15.0.1",
    "@react-native-community/cli-platform-android": "15.0.1",
    "@react-native-community/cli-platform-ios": "15.0.1",
    "@react-native/babel-preset": "0.76.5",
    "@react-native/eslint-config": "0.76.5",
    "@react-native/metro-config": "0.76.5",
    "@react-native/typescript-config": "0.76.5",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "detox": "^20.44.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-detox": "^1.0.0",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.6.3",
    "jest-html-reporter": "^4.3.0",
    "open-cli": "^8.0.0",
    "prettier": "^3.6.2",
    "react-test-renderer": "18.3.1",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
